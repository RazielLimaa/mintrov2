{
  "expo": {
    "name": "VITACARE",
    "slug": "vitacare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vitacare",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa acessar suas fotos para permitir que vocÃª selecione um avatar."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "splash": {

      "backgroundColor": "#8A2BE2"
    }
  }
}